<Application x:Class="Application"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:NSTARMC_Tools"
    StartupUri="beta_verify.xaml"
    xmlns:ui="http://schemas.modernwpf.com/2019"
    ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WPFUI;component/Styles/Theme/Dark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/WPFUI;component/Styles/WPFUI.xaml" />
                <ui:ThemeResources />
                <ui:XamlControlsResources />
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="iconfont">pack://application:,,,/NSTARMC-Tools;component/res/#iconfont</FontFamily>
            <FontFamily x:Key="SegoeFluentIcons">pack://application:,,,/NSTARMC-Tools;component/res/#Segoe Fluent Icons</FontFamily>
            <FontFamily x:Key="Segoe Fluent Icons">pack://application:,,,/NSTARMC-Tools;component/res/#Segoe Fluent Icons</FontFamily>
            <FontFamily x:Key="Segoe MDL2 Assets">pack://application:,,,/NSTARMC-Tools;component/res/#Segoe MDL2 Assets</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
